Package: carpentR
Title: Predict Lake Algal Blooms Using Plankton Dynamics
Version: 0.0.0.9000
Authors@R: c(
    person(given = "Simon",
           family = "Couch",
           role = c("aut", "cre"),
           email = "simonpatrickcouch@gmail.com"),    
